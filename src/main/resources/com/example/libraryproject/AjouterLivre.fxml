<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootPane" prefHeight="232.0" prefWidth="379.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.libraryproject.AjouterLivreController">
   <children>
      <VBox prefHeight="289.0" prefWidth="492.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextField fx:id="Titre" promptText="Titre">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="Auteur" layoutX="20.0" layoutY="30.0" promptText="Auteur">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="ISBN" layoutX="20.0" layoutY="30.0" promptText="ISBN">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <HBox prefHeight="47.0" prefWidth="492.0">
               <children>
                  <Button fx:id="saveButton" mnemonicParsing="false" onAction="#addBook" prefHeight="47.0" prefWidth="189.0" stylesheets="@AjouterLivre.css" text="Enregister" />
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" prefHeight="47.0" prefWidth="194.0" stylesheets="@AjouterLivre.css" text="Annuler" />
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
