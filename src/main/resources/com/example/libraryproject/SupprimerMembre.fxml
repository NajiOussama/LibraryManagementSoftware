<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootPane" prefHeight="244.0" prefWidth="379.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.libraryproject.SupprimerMembreController">
    <children>
        <VBox prefHeight="244.0" prefWidth="379.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <TextField fx:id="nom" promptText="Nom">
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="prenom" layoutX="20.0" layoutY="30.0" promptText="Prénom">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="20.0" />
                    </VBox.margin>
                </TextField>
            <TextField fx:id="StudentNumber" prefHeight="26.0" prefWidth="367.0" promptText="Numéro Etudiant">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextField>
                <HBox prefHeight="47.0" prefWidth="492.0">
                    <children>
                        <Button fx:id="saveButton" mnemonicParsing="false" onAction="#removeMember" prefHeight="47.0" prefWidth="189.0" stylesheets="@AjouterLivre.css" text="Enregister" />
                        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" prefHeight="47.0" prefWidth="194.0" stylesheets="@AjouterLivre.css" text="Annuler" />
                    </children>
                    <VBox.margin>
                        <Insets top="30.0" />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
